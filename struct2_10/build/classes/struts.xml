<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>
	<package name="welcome" namespace="/" abstract="false" extends="struts-default">
			<action name="input" class="cn.com.zjf.action.ConversionAction" method="execute">
				<result>/index.jsp</result>
				<result name="input">/input.jsp</result>
			</action>
			<action name="testConnection" class="cn.com.zjf.action.TestConnectionAction">
				<result>/success.jsp</result>
			</action>
	</package>
</struts>
